//  297   uint8          countLow, countHigh;
//  298   halIntState_t  s;
//  299 
//  300   HAL_ENTER_CRITICAL_SECTION(s);
        MOV     A,0xa8
        MOV     R0,A
        CLR     0xa8.7
//  301   MAC_MCU_T2_ACCESS_COUNT_VALUE();
