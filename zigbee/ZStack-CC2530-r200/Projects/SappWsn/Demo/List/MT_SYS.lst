    435                pRetBuf[1] = 0;
    436              }
    437          
    438              /* Build and send back the response */
    439              MT_BuildAndSendZToolResponse(((uint8)MT_RPC_CMD_SRSP | (uint8)MT_RPC_SYS_SYS),
    440                                             MT_SYS_OSAL_NV_READ, respLen, pRetBuf );
    441          
    442              osal_mem_free(pRetBuf);
    443            }
