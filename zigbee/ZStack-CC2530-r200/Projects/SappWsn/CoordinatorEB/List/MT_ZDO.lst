   1973              *pTmp++ = HI_UINT16(pMsg->nwkAddr);
   1974          
   1975              *pTmp++ = pMsg->startIndex;
   1976              *pTmp++ = listLen;
   1977          
   1978              MT_Word2Buf(pTmp, pMsg->devList, listLen);
   1979          
   1980              MT_BuildAndSendZToolResponse(((uint8)MT_RPC_CMD_AREQ | (uint8)MT_RPC_SYS_ZDO), id, len, pBuf);
