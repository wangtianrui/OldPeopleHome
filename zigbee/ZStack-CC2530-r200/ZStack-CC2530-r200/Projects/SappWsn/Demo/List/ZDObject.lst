    424              else
    425              {
    426                pMsg->event = ZDO_STATE_CHANGE;
    427                pMsg->status = state;
    428          
    429                (void)osal_msg_send(taskId, (uint8 *)pMsg);
    430              }
    431            }
    432            else
    433            {
    434              // Modify in place the status of an existing ZDO_STATE_CHANGE message to the EndPoint.
    435              pMsg->status = state;
